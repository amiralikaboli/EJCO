		let interm0_trie1 = interm0_trie0(x0)
		sum(<x1, interm0_trie2> in interm0_trie1) {
			if (x1 in lt_trie0) {
				let lt_trie1 = lt_trie0(x1)
				sum(<x2, mk_trie2> in mk_trie1) {
					if (x2 in k_trie0) {
						let k_trie1 = k_trie0(x2)
						let mn_interm0_col1 = sum(<interm0_off, _> in interm0_trie2) { interm0_col1[interm0_off] }
						let mn_interm0_col2 = sum(<interm0_off, _> in interm0_trie2) { interm0_col2[interm0_off] }
						let mn_interm0_col4 = sum(<interm0_off, _> in interm0_trie2) { interm0_col4[interm0_off] }
						let mn_lt_link = sum(<lt_off, _> in lt_trie1) { lt_link[lt_off] }
					}
