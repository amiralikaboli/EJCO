let interm0_trie0 = sum(<x0, t1_trie1> in t1_trie0) {
	if (x0 in ml_trie0) {
		let ml_trie1 = ml_trie0(x0)
		sum(<x1, ml_trie2> in ml_trie1) {
			if (x1 in t2_trie0) {
				let t2_trie1 = t2_trie0(x1)
				sum(<t1_off, _> in t1_trie1) {
					sum(<ml_off, _> in ml_trie2) {
						sum(<t2_off, _> in t2_trie1) {
							{ <t1_id[t1_off], t1_title[t1_off], ml_linked_movie_id[ml_off], ml_link_type_id[ml_off], t2_title[t2_off]> -> true }
						}
					}
				}
			}
		}
	}
}
